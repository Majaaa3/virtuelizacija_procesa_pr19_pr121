<?xml version="1.0" encoding="utf-8"?>
<configuration>
	<appSettings>
		<!-- ROOT do tvog dataset-a; npr. ...\Dataset\B01\EIS measurements\Test_1\Hioki -->
		<add key="DatasetRoot" value=".\Dataset"/>
		<add key="LogPath" value=".\client-import.log"/>
	</appSettings>

	<system.serviceModel>
		<client>
			<endpoint name="EisIngest"
					  address="net.tcp://localhost:4000/EisIngest"
					  binding="netTcpBinding"
					  bindingConfiguration="tcpLarge"
					  contract="Battery.Common.IEisIngestService"/>
		</client>

		<bindings>
			<netTcpBinding>
				<binding name="tcpLarge" transferMode="Streamed"
						 maxReceivedMessageSize="10485760"
						 receiveTimeout="00:10:00" sendTimeout="00:10:00">
					<security mode="None"/>
				</binding>
			</netTcpBinding>
		</bindings>
	</system.serviceModel>
</configuration>
